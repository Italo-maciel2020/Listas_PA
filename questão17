#include <stdio.h>
#include <stdlib.h>

void completa_vetor(int *v,int n){
    v = malloc(n * sizeof(int));
    printf("Insira os elementos do vetor.");
    for(int i=0;i<n;i++){
        scanf("%d", &v[i]);
    }
    free(v);
}

void soma_vetores(int *v1,int *v2,int *v3,int n){
    v3 = malloc(n * sizeof(int));
    for(int i=0;i<n;i++){
        v3[i] = v1[i] + v2[i];
    }
}

int main()
{
    int *v1,*v2;
    int n;
    printf("Insira o tamanho dos vetores.");
    scanf("%d",&n);
    completa_vetor(v1,n);
    completa_vetor(v2,n);
    soma_vetores(v1,v2,v3,n);
    for(int i=0;i<n;i++){
        printf("%d", v3[i]);
    }
    return 0;
}
